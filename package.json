{
  "name": "nuxtjs-template",
  "version": "1.0.0",
  "description": "",
  "author": "Curtis Belt <curtis@curtisbelt.dev>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "format": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "release": "standard-version",
    "dev": "npm-run-all --parallel dev:**",
    "dev:app": "nuxt",
    "dev:functions": "netlify-lambda serve functions -c ./webpack.functions.js",
    "prepare": "npm-run-all --parallel prepare:**",
    "prepare:app": "nuxt generate",
    "prepare:functions": "netlify-lambda build functions -c ./webpack.functions.js",
    "start": "npm-run-all --parallel start:**",
    "start:app": "nuxt start",
    "start:functions": "netlify-lambda serve functions -c ./webpack.functions.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/netlify-files": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.1.2",
    "cookie-universal-nuxt": "^2.0.18",
    "faunadb": "^2.8.1",
    "nuxt": "^2.10.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nuxtjs/eslint-config": "1.1.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.0.3",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.9",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "standard-version": "^7.0.1"
  }
}
